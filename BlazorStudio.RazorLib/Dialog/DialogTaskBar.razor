@inherits Fluxor.Blazor.Web.Components.FluxorComponent

<div class="bstudio_dialog-task-bar">
    <BlazorStudio.RazorLib.TaskModelManager.TaskModelManagerDialogEntryPointDisplay/>

    @{
        var dialogStates = DialogStatesWrap.Value;

        foreach (var dialog in dialogStates.List)
        {
            <DialogTaskBarEntry DialogRecord="dialog"/>
        }
    }
</div>